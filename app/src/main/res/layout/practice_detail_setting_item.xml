<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="practiceRowItem"
            type="jp.ceed.kart.settings.model.dto.PracticeDetailAdapterItem.PracticeRowItem" />
    </data>

    <jp.ceed.kart.settings.ui.practice.view.PracticeSettingRowView
        android:id="@+id/practiceRowView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:practiceRowItem="@{practiceRowItem}"/>
</layout>